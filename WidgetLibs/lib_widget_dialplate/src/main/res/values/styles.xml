<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="dial_button">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:textColor">@drawable/text_color_dial_number_selector</item>
        <item name="android:textSize">33sp</item>
        <item name="android:gravity">center</item>
        <item name="android:focusable">true</item>
        <item name="android:clickable">true</item>
        <item name="android:background">@drawable/view_dial_number_selector</item>
    </style>

    <style name="dial_line_v">
        <item name="android:layout_width">17.3dp</item>
        <item name="android:layout_height">match_parent</item>
    </style>

    <style name="dial_line_h">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">13.3dp</item>
    </style>

    <style name="dial_item">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">80dp</item>
        <item name="android:clipChildren">false</item>
    </style>

    <style name="dial_image_view">
        <item name="android:focusable">true</item>
        <item name="android:clickable">true</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">1</item>
    </style>
</resources>